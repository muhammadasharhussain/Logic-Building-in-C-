void reverse(int *arr,int L,int R){
    while(L<R){
        int temp=arr[L];
        arr[L]=arr[R];
        arr[R]=temp;
        L++;
        R--;
    }
}
#include <stdio.h>
int main(){
    int n,k;
    printf("Enter length of array: ");
    scanf("%d",&n);
    printf("Enter elements of array: ");
    int hehe[n];
    for(int i=0;i<n;i++)
        scanf("%d",&hehe[i]);
        
    printf("Enter No. of rotations: ");
    scanf("%d",&k);
    
    k=k%n;
    reverse(hehe,0,n-1);
    reverse(hehe,0,k-1);
    reverse(hehe,k,n-1);

    for(int i=0;i<n;i++)
    printf("%d ",hehe[i]);
    return 0;
}
