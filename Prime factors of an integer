#include <stdio.h>
void factors(int n){
int count=0;
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            count=0;
            for(int j=2;j<=i/2;j++){
                if(i%j==0)
                    count++;
                }
            if(count==0)
            printf("%d ",i);
        }
    }
}
int main(){
    int z;
    printf("Enter a number: ");
    scanf("%d",&z);
    factors(z);
    return 0;
}
