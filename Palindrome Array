#include<stdio.h>
int main(){
int n;
printf("Enter length of array: ");
scanf("%d",&n);
int a[n];
printf("\nEnter elements of array: ");
for(int i=0;i<n;i++)
scanf("%d",&a[i]);
int L=0;
int R=sizeof(a)/sizeof(a[0]) - 1;
int flag=0;
while(L<R){
    if(a[L]!=a[R]){
        flag=1;
        break;
    }
    L++;
    R--;
}
if(flag)
printf("Not a Palindrome");
else
printf("Palindrome");
return 0;}
