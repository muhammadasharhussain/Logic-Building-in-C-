#include <stdio.h>
int main(){
    int temp,i,n;
    printf("Size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("Enter Elements of array: ");
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    int L=0;
    int R=sizeof(arr)/sizeof(arr[0])-1;
    while(L<R){
        temp=arr[L];
        arr[L]=arr[R];
        arr[R]=temp;
        L++;
        R--;
    }
    for(i=0;i<n;i++)
    printf("%d ",arr[i]);
}
