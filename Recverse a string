#include <stdio.h>
#include <string.h>
int i=0;
char temp;
int main() {
    char str[250];
    printf("Enter a String: ")
    scanf("%s",str);
    int j=strlen(str)-1;
    while(i<j){
        temp=str[i];
        str[i]=str[j];
        str[j]=temp;
        i++;
        j--;
    }
    printf("Reversed string is: ")
    puts(str);
    return 0;
}
